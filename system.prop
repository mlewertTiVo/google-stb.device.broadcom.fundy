# gfx setup (gles 3.1)
ro.opengles.version=196609
debug.hwui.render_dirty_regions=false
ro.nx.mma=1

# middleware heaps configuration
ro.nx.heap.video_secure=80m
ro.nx.heap.main=96m
ro.nx.heap.drv_managed=0m
ro.nx.heap.grow=2m
ro.nx.heap.shrink=2m
ro.nx.heap.gfx=64m
ro.nx.capable.dtu=1

# hwc optimizations
ro.nx.capable.cb=1
ro.nx.capable.bg=1
ro.sf.lcd_density=320

ro.nx.eth.irq_mode_mask=f:c

# google assigned client id
ro.com.google.clientidbase=android-acme

dyn.nx.hdcp.force=0

